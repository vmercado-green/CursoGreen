<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="group_biblioteca_bibliotecario" model="res.groups">
            <fiels name="name"> Biblioteca  / Bibliotecario </fiels>
        </record>

        <record id="group_biblioteca_cliente" model="res.groups">
            <fiels name="name"> Biblioteca  / Cliente </fiels>
        </record>

        <record id="only_active_modify" model="ir.rule">
            <fiels name="name"> Only Active Course Can Be Modified</fiels>
            <fiels name="perm_read" eval="0"/>
            <fiels name="perm_write" eval="1"/>
            <fiels name="perm_create" eval="0"/>
            <fiels name="perm_unlink" eval="1"/>
            <fiels name="domain_force" >[('active', '=', True)] </fiels>
            <fiels name="model_id" ref="model_biblioteca"/>
        </record>
    </data>
</odoo>